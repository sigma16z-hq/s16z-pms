# Database Configuration (using non-conflicting port 5433)
DATABASE_URL="postgresql://postgres:postgres@localhost:5433/s16z_pms?schema=public"

# Redis Configuration (using non-conflicting port 6380)
REDIS_URL="redis://localhost:6380"

# Application Configuration
NODE_ENV=development
PORT=3000

# External API Configuration

# HRP (Hidden Road Partners) API Configuration
# Authentication endpoint for OAuth2 token requests
HRP_AUTH_BASE_URL=https://auth.hiddenroad.com
# Data API endpoint for account activity, trades, etc.
HRP_DATA_BASE_URL=https://api.hiddenroad.com
# OAuth2 client credentials for HRP API access
HRP_CLIENT_ID=your_hrp_client_id_here
HRP_CLIENT_SECRET=your_hrp_client_secret_here
# OAuth2 audience (usually the data API URL + /v0/)
HRP_AUDIENCE=https://api.hiddenroad.com/v0/

# 1Token API Configuration
# Base URL for 1Token API endpoints
ONETOKEN_BASE_URL=https://www.1tokencam.com/api/v1
# API key for 1Token authentication
ONETOKEN_API_KEY=your_onetoken_api_key_here
# API secret for HMAC signature generation (base64 encoded)
ONETOKEN_API_SECRET=your_onetoken_api_secret_here

# Optional: SOCKS5 Proxy Configuration (for both APIs)
# Uncomment and configure if you need proxy support
# PROXY_HOST=your_proxy_host
# PROXY_PORT=1080
# PROXY_USERNAME=your_proxy_username
# PROXY_PASSWORD=your_proxy_password

