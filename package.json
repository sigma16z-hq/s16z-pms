{
  "name": "s16z-pms",
  "version": "0.0.1",
  "description": "S16Z Portfolio Management System - NestJS Migration",
  "author": "S16Z Team",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/pms-service/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/pms-service/test/jest-e2e.json",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:generate": "prisma generate --schema=libs/database/prisma/schema.prisma",
    "db:push": "prisma db push --schema=libs/database/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=libs/database/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=libs/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "5.7.1",
    "cache-manager": "^7.2.0",
    "ioredis": "^5.3.2",
    "node-fetch": "^3.3.2",
    "openapi-fetch": "^0.10.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socks-proxy-agent": "^8.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/node": "^20.3.1",
    "prisma": "5.7.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3"
  }
}